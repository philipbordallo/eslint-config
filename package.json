{
  "name": "eslint-configs",
  "version": "3.2.0",
  "private": true,
  "main": "build.js",
  "license": "MIT",
  "author": "Philip Bordallo",
  "scripts": {
    "build": "babel-node build.js",
    "prerelease": "npm run build",
    "release": "npm-run-all publish:*",
    "publish:default": "npm publish packages/eslint-config",
    "publish:es5": "npm publish packages/eslint-config-es5",
    "publish:react": "npm publish packages/eslint-config-react",
    "publish:typescript": "npm publish packages/eslint-config-typescript",
    "publish:typescript-react": "npm publish packages/eslint-config-typescript-react",
    "version": "npm run version:packages && git add .",
    "version:packages": "babel-node version.js",
    "test": "jest --config jest.config.js",
    "clean": "rimraf packages/**/index.js"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "jest": true
    },
    "rules": {
      "no-console": "off"
    },
    "extends": "./packages/eslint-config/index.js"
  },
  "dependencies": {
    "eslint-restricted-globals": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@types/jest": "^26.0.9",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
