{
  "name": "eslint-configs",
  "version": "2.3.0",
  "private": true,
  "main": "build.js",
  "license": "MIT",
  "author": "Philip Bordallo",
  "scripts": {
    "build": "babel-node build.js",
    "prerelease": "npm run build",
    "release": "npm-run-all publish:*",
    "publish:default": "npm publish packages/eslint-config",
    "publish:es5": "npm publish packages/eslint-config-es5",
    "publish:react": "npm publish packages/eslint-config-react",
    "version": "npm run version:packages && git add .",
    "version:packages": "babel-node version",
    "clean": "rimraf packages/**/index.js"
  },
  "dependencies": {
    "eslint-restricted-globals": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  }
}
